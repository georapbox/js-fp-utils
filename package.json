{
  "name": "js-fp-utils",
  "version": "0.1.0",
  "description": "Javascript utilities for functional programming",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "jest",
    "test:w": "jest --watch",
    "build": "babel src/**/*.js -d lib --ignore 'src/**/*.test.js'",
    "prepush": "npm-run-all --parallel test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georapbox/js-fp-utils.git"
  },
  "keywords": [
    "javascript",
    "functional-programming",
    "utilities"
  ],
  "author": "George Raptis <georapbox@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georapbox/js-fp-utils/issues"
  },
  "homepage": "https://github.com/georapbox/js-fp-utils#readme",
  "devDependencies": {
    "@babel/cli": "~7.7.5",
    "@babel/core": "~7.7.5",
    "@babel/preset-env": "~7.7.6",
    "eslint": "~6.6.0",
    "husky": "~3.1.0",
    "jest": "~24.9.0",
    "npm-run-all": "~4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  }
}
